node {
    stage('SCM') {
        echo 'Gathering code from version control'
        echo 'Building new feature'
    }

    stage('Build') {
        echo 'Building...'
        sh 'dotnet --version'
    }
    
    stage('Test') {
        echo 'Testing...'
    }
    
    stage('Deploy') {
        echo 'Deploying...'
    }
}